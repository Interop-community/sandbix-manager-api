{
  "executionRoleArn": "arn:aws:iam::657600230790:role/ecsTaskExecutionRole",
  "family": "sandbox-manager-test",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "sandbox-manager-test",
      "image": "hspconsortium/sandbox-manager-api:0.7.0-SNAPSHOT",
      "cpu": 512,
      "memoryReservation": 1024,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/sandbox-manager-test",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 12000,
          "protocol": "tcp",
          "containerPort": 12000
        }
      ],
      "environment": [
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
